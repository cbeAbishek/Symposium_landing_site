{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true,
      "*": false
    }
  }
}
